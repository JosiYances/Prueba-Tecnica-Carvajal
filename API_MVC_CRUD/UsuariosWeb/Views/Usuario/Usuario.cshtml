@model UsuarioT
@{
    ViewBag.Title = "Usuario";
}

<h2>Usuario</h2>

<hr />
<a href="@Url.Action("Index", "Home")">Regresar al Inicio</a>
<br />
<hr />

<div class="form-group">
    <br />
    <label> Nombre:</label>
    @Model.Nombre
</div>

<div class="form-group">
    <br />
    <label> Apellido:</label>
    @Model.Apellido
</div>

<div class="form-group">
    <br />
    <label> Tipo de Documento:</label>
    @Model.TipoID
</div>

<div class="form-group">
    <br />
    <label> Número de Documento:</label>
    @Model.Identificación
</div>


<div class="form-group">
    <br />
    <label> Contraseña:</label>
    @Model.Contrasena
</div>


<div class="form-group">
    <br />
    <label> Correo:</label>
    @Model.Correo
</div>

<a href="@Url.Action("Editar", "Editar", new {id = Model.Id})">Editar Usuario</a>
<a href="@Url.Action("Eliminar", "Eliminar", new {id = Model.Id})">Eliminar Usuario</a>





